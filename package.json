{
  "name": "ng-to-vue",
  "version": "1.0.0",
  "description": " A demo of a Vue.js app built by refactoring an AngularJS 1 app",
  "author": "Gene Huh <ehuh@teamexos.com>",
  "private": true,
  "scripts": {
    "start": "nf start",
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "jest",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "mock-api": "json-server --watch ./api/db.json --routes ./api/routes.json --port 3003",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o .out"
  },
  "dependencies": {
    "@storybook/addon-knobs": "^3.3.15",
    "axios": "^0.17.1",
    "babel-runtime": "^6.22.0",
    "bootstrap-vue": "^2.16.0",
    "vue": "^2.5.9",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.0-rc.2",
    "@storybook/addon-centered": "^3.4.0-rc.2",
    "@storybook/addon-links": "^3.4.0-rc.2",
    "@storybook/addon-viewport": "^3.4.0-rc.2",
    "@storybook/addons": "^3.4.0-rc.2",
    "@storybook/vue": "^3.4.0-rc.2",
    "autoprefixer": "^6.7.2",
    "avoriaz": "^6.0.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-preset-vue": "^2.0.1",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "chromedriver": "^2.27.2",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^3.1.4",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.2",
    "file-loader": "^0.10.0",
    "foreman": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "function-bind": "^1.1.0",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "inject-loader": "^2.0.1",
    "jest": "^23.0.0-alpha.0",
    "jest-vue-preprocessor": "^1.4.0",
    "json-loader": "^0.5.4",
    "json-server": "^0.12.1",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^2.0.2",
    "lolex": "^1.5.2",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.12",
    "opn": "^4.0.2",
    "ora": "^1.1.0",
    "phantomjs-prebuilt": "^2.1.14",
    "react-test-renderer": "^16.2.0",
    "selenium-server": "^3.0.1",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.3.0",
    "vue-server-renderer": "^2.5.9",
    "vue-star-rating": "^1.4.0",
    "vue-style-loader": "^2.0.0",
    "vue-template-compiler": "^2.5.9",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-merge": "^2.6.1"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "vue",
      "node",
      "json"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@storybook/.*\\.vue$))"
    ],
    "moduleNameMapper": {
      "src/components/(.*)$": "<rootDir>/src/components/$1",
      "^vue$": "vue/dist/vue.common.js",
      "assets": "<rootDir>/src/assets",
      "components": "<rootDir>/src/components",
      "store": "<rootDir>/src/store"
    }
  }
}
